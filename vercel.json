{
  "version": 2,
  "builds": [
    {
      "src": "next.config.js",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "NEXT_PUBLIC_SITE_URL": "https://doctor-appointment-dashboard.vercel.app"
  },
  "github": {
    "silent": true
  }
}
